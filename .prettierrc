{
  "printWidth": 80, //  줄 바꿈 할 폭 길이
  "trailingComma": "all", // 여러 줄을 사용할 때, 후행 콤마 사용 방식
  "tapWidth": 2, // 탭 너비
  "printWidth": 200,
  "semi": true,
  "rangeStart": 0,
  "singleQuote": true, // single 쿼테이션 사용 여부
  "jsxBracketSameLine": false, // JSX의 마지막 `>`를 다음 줄로 내릴지 여부
  //settings.json
  "editor.formatOnSave": true
}
